{
  "name": "console.freelog.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "dev": "vue-cli-service serve",
    "build:prod": "npm run build -- --env=prod",
    "build:beta": "npm run build -- --env=beta",
    "pack": "tar -zcf console.tar dist",
    "test:e2e": "vue-cli-service test:e2e --config ./nightwatch.conf.js",
    "test:e2e:chrome": "npm run test:e2e -- --env chrome",
    "test:e2e:safari": "npm run test:e2e -- --env safari",
    "test:e2e:firefox": "npm run test:e2e -- --env firefox",
    "test:e2e:all": "npm run test:e2e -- --env chrome,firefox,safari"
  },
  "husky": {
    "hooks": {
      "pre-push": "freelog-ci deploy"
    }
  },
  "dependencies": {
    "@freelog/freelog-ci": "^0.0.1",
    "@freelog/freelog-ui-contract": "^0.0.15",
    "@freelog/resource-policy-lang": "^0.0.33",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-e2e-nightwatch": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.4",
    "@vue/component-compiler-utils": "^3.0.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "ali-oss": "^6.1.1",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "chromedriver": "^75.0.0",
    "co": "^4.6.0",
    "codemirror": "^5.48.0",
    "consola": "^2.9.0",
    "cropperjs": "^1.5.1",
    "crypto-js": "^3.1.9-1",
    "date-fns": "^1.30.1",
    "element-ui": "^2.10.0",
    "eslint-config-elemefe": "^0.3.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-json": "^1.4.0",
    "font-awesome": "^4.7.0",
    "geckodriver": "^1.16.2",
    "husky": "^2.7.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.11",
    "lozad": "^1.9.0",
    "nprogress": "^0.2.0",
    "ora": "^3.4.0",
    "quill": "^1.3.6",
    "screenfull": "^4.2.0",
    "selenium-server": "^3.141.59",
    "store": "^2.0.12",
    "user-home": "^2.0.0",
    "vue": "^2.6.10",
    "vue-codemirror": "^4.0.6",
    "vue-i18n": "^8.11.2",
    "vue-lazyload": "^1.3.0",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0",
    "walk": "^2.3.14"
  }
}
